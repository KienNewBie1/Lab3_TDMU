@model IEnumerable<SachOnline.Models.Publisher>
<ul class="list-group">
    <li class="list-group-item active" style="font-weight:bold; text-align:center">SÁCH THEO NHÀ XUẤT BẢN</li>
    @foreach (var item in Model)
    {
        <a href="@Url.Action("SachTheoNhaXuatBan","SachOnline",new {id = item.Publisher_id})" class="list-group-item">
            @item.Publisher_name
        </a>
    }
</ul>
